{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-yannam"
		},
		"yannamadfdlksals_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'yannamadfdlksals'"
		},
		"yannamadfsals_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'yannamadfsals'"
		},
		"covid_eu_http_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://opendata.ecdc.europa.eu"
		},
		"yannamadfdlksals_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://yannamadfdlkgen2.dfs.core.windows.net/"
		},
		"trigger-sc_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c48564fc-efbd-4ccb-af21-b591537f3f1a/resourceGroups/rg-dev-cus-log-archive-logs/providers/Microsoft.Storage/storageAccounts/yannamadfsa"
		},
		"hospidata_trigger_properties_plfor_covid_deaths_ingest_parameters_SourceRelativeURL": {
			"type": "string",
			"defaultValue": "en/publications-data/covid-19-data-14-day-age-notification-rate-new-cases"
		},
		"hospidata_trigger_properties_plfor_covid_deaths_ingest_parameters_sinkfileName": {
			"type": "string",
			"defaultValue": "hospi_data.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-yannam_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"yannamadfdlksals_accountKey": {
						"value": "[parameters('yannamadfdlksals_accountKey')]"
					},
					"yannamadfsals_connectionString": {
						"value": "[parameters('yannamadfsals_connectionString')]"
					},
					"covid_eu_http_ls_properties_typeProperties_url": {
						"value": "[parameters('covid_eu_http_ls_properties_typeProperties_url')]"
					},
					"yannamadfdlksals_properties_typeProperties_url": {
						"value": "[parameters('yannamadfdlksals_properties_typeProperties_url')]"
					},
					"trigger-sc_properties_typeProperties_scope": {
						"value": "[parameters('trigger-sc_properties_typeProperties_scope')]"
					},
					"hospidata_trigger_properties_plfor_covid_deaths_ingest_parameters_SourceRelativeURL": {
						"value": "[parameters('hospidata_trigger_properties_plfor_covid_deaths_ingest_parameters_SourceRelativeURL')]"
					},
					"hospidata_trigger_properties_plfor_covid_deaths_ingest_parameters_sinkfileName": {
						"value": "[parameters('hospidata_trigger_properties_plfor_covid_deaths_ingest_parameters_sinkfileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}